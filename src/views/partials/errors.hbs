{{#each errors}}
  {{!-- <div class="alert alert-danger" role="alert">
        <strong>Error!</strong> {{this}}
    </div> --}}

  <div class="alert alert-danger alert-dismissible fade show" role="alert">
    {{text}}
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    >
      <span aria-hidden="true">&times;</span>
    </button>

  </div>

{{/each}}